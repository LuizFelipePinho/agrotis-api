// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Empresa {
  id           String   @id @default(uuid())
  razaoSocial  String
  CNPJ         String   @unique
  email        String   @unique
  dataAbertura DateTime
  CEP          Int
  endereco     String
  numero       Int
  complemento  String?
  bairro       String
  cidade       String
  UF           String
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}
